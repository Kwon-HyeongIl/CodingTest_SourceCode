class Solution {
    public String solution(String n_str) {
        
        StringBuilder sb = new StringBuilder(n_str);
        int ln = sb.length();
        
        for (int i=0; i<ln; i++) {
            if (sb.charAt(0) == '0') {
                sb.deleteCharAt(0);
            } else {
                break;
            }
        }
        
        return sb.toString();
    }
}
