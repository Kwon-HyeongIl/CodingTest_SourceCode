import java.util.*;

class Solution {
    public int[] solution(int[] num_list) {
        
        List<Integer> list = new ArrayList<>();
        
        for (int i=num_list.length-1; i>=0; i--) {
            list.add(num_list[i]);
        }
        
        return list.stream().mapToInt(o->o).toArray();
    }
}
